<h1>Onboarding Application</h1>
<div class="center">
  <%= form_for(:submerchant, :html => {class: "center", role: "form"}) do |f| %>
    <div class="center">
      <%= f.label "submerchant ID" %>
      <%= f.text_field :id, class: "middle" %>
    </div>

    <div class="center">
      <%= f.label "first name" %>
      <%= f.text_field :first_name, class: "middle" %>
    </div>
    <%= f.submit %>
  <% end %>
</div>
<script src="https://js.braintreegateway.com/web/3.18.0/js/client.min.js"></script>
